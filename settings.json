{
	"version": "0.3.7",
	"headings": {
		"title": "Document Pager",
		"h1": "Document Pager",
		"index": "Content",
		"content": "Content"
	},
	"languages": {
		"javascript": {
			"extensions": ["js","javascript"],
			"breaks": { "major": ["/**"], "minor": ["/*"]},
			"highlight": "//!"
		},
		"php": {
			"extensions": ["php"],
			"breaks": { "major": ["/**","/*:"], "minor": ["/*"] },
			"highlight": "//!"
		},
		"python": {
			"extensions": ["py"],
			"breaks": { "major": ["#:"], "minor": ["#::"] },
			"highlight": "#!"
		},
		"sql": {
			"extensions": ["sql"],
			"breaks": { "major": ["/**"], "minor": ["/*"] },
			"highlight": "--!"
		},
		"css": {
			"extensions": ["css"],
			"breaks": { "major": ["/**"], "minor": ["/*"] },
			"highlight": "/*!*/"
		},
		"markdown": {
			"extensions": ["md","markdown","mdzip"],
			"breaks": { "major": ["##"]}
		},
		"text": {
			"extensions": ["txt","text","*"],
			"breaks": { "major": ["#"], "minor": ["##"] },
			"highlight": "#!"
		}
	}
}
