{
	"version": "0.2.0",
	"headings": {
		"title": "Document Pager",
		"h1": "Document Pager",
		"index": "Content",
		"content": "Content"
	},
	"breaks": {
		"js": { "major": "/**", "minor": "/*"},
		"php": { "major": ["/**","/*:"], "minor": "/*" },
		"javascript": { "major": "/**", "minor": "/*" },
		"sql": { "major": "/**", "minor": "/*" },
		"css": { "major": "/**", "minor": "/*" },
		"md": { "major": "##"},
		"markdown": { "major": "##", "minor": "###" },
		"*": { "major": "##", "minor": "###" }
	}
}
